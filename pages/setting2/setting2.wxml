        <a class="weui-btn weui-btn_mini weui-btn_default" data-field="FE" bind:tap="saveFun">保存</a>
        <a data-page="setting2" data-key="topTip8" bindtap="showTopTip" class="weui-btn weui-btn_mini weui-btn_default">
          <mp-icon icon="help" color="black" size="{{22}}"></mp-icon>
        </a>
      </view>
    </mp-cell>
  </mp-cells>
  <!-- <button bindtap="logOut">退出登录</button> -->
</view>